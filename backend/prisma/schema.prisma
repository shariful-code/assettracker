datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  
}

model User {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  email     String   @unique
  password  String
  phone     String   @unique
  token      String @default("")
  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
